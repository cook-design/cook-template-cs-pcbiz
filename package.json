  "description": "辰森PC业务组件模板",
  "author": "yongbao.fyb",
  "homepage": "",
  "files": [
    "document",
    "src",
    ".babelrc",
    ".gitignore",
    ".eslintrc",
    ".eslintignore",
    ".stylelintrc",
    ".stylelintignore",
    "__.gitignore",
    "index.d.ts"
  ],
  "mentorConfig": {
    "solution": "cs-pcbiz",
    "id": "",
    "platform": "",
    "tag": "",
    "type": ""
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "es/index.js",
  "devDependencies": {
    "antd": "3",
    "babel-cli": "6",
    "babel-plugin-add-module-exports": "0",
    "babel-plugin-import": "1",
    "babel-preset-env": "1",
    "babel-preset-react": "6",
    "babel-preset-stage-0": "6",
    "classnames": "2",
    "cross-env": "5",
    "eslint": "4",
    "eslint-config-cook": "1",
    "lesshint": "2",
    "pre-commit": "1",
    "prop-types": "15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "2",
    "stylelint": "7",
    "stylelint-config-standard": "16"
  },
  "peerDependencies": {
    "classnames": ">=2.0.0",
    "prop-types": ">=15.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "antd": ">=3.0.0"
  },
  "scripts": {
    "lint": "npm run srclint && npm run demolint && npm run stylelint",
    "srclint": "eslint site src scripts --ext '.js,.jsx'",
    "demolint": "cross-env RUN_ENV=DEMO eslint document/**/*.md --ext '.md'",
    "stylelint": "stylelint src/**/*.less --syntax less",
    "copydts": "cp index.d.ts lib/",
    "compile": "rimraf ./lib & rimraf ./es && npm run compile:js && npm run compile:es",
    "compile:js": "cross-env BABEL_ENV=common babel src --out-dir ./lib --copy-files",
    "compile:es": "cross-env BABEL_ENV=es babel src --out-dir ./es --copy-files"
  },
  "pre-commit": [
    "lint"
  ]
}
